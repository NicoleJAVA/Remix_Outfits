package remix;
import java.util.*;

public class test {

    int[][] x;
    x = new int[10][]; // 先產生x陣列
    for (int i = 0; i < x.length; i++) {
        x[i] = new int[i]; // 再分別產生x[i]所指到的陣列
    }
    for (int i = 0; i < x.length; i++) {
        for (int j = 0; j < x[i].length; j++) {
            x[i][j] = i + j;
    		System.out.print(" "+ x[i][j]);
    		
        }
        System.out.println("\n");
    }
    int[] s = {1, 2,5,66, 11}; 
    s[s.length+1] = new int() ;
	for (int k=0; k<s.length; k++){
		System.out.print(" " + s[k]);
	}

	
}
