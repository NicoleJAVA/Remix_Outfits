package remix;

import java.util.ArrayList;

public class test_addfakeoutfit {

	
	
     ArrayList<Object> Items; 
    
	 public void addItem( Object item ){
		 
		 this.Items.add(item);
		
	}

	
	public test_addfakeoutfit() {

		System.out.println("public");
		Items = new ArrayList<Object>(); 
 		blouse fakeblouse = new blouse(); 
		skirt fakeskirt = new skirt();  
		shoe fakeshoe = new shoe();  

		
		fakeblouse.shape = new String("aline");
		fakeblouse.print = new String("plain");
		fakeblouse.material = new String("cotton");
		fakeblouse.append = new String("none"); 
		fakeblouse.black = true; 	
		fakeblouse.white = true; 	
		
		fakeskirt.shape = new String("aline");
		fakeskirt.print = new String("plain");
		fakeskirt.material = new String("cotton");
		fakeskirt.append = new String("none"); 
		fakeskirt.black = true; 
		
		
		this.addItem(fakeblouse); 
		this.addItem(fakeskirt); 
		this.addItem(fakeshoe); 
		
		
		
		
		
		
	}

}
